<h1>All events</h1>



<p><%= link_to "Add Event", new_event_path  %></p>

<% @events.each do |e| %>
	<div>
		<p><h4> <%= link_to e.title, event_path(e) %></h4></p>
		<p> <%= time_format(e.start_time) %></p>
		<p> <%= time_format(e.end_time) %></p>
		<p> <%= e.location %>, <%= e.venue %></p>
	</div>
<% end %>
